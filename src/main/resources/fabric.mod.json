{
  "schemaVersion": 1,
  "id": "exumenchantment",
  "version": "${version}",
  "name": "Exum Enchantment",
  "description": "A way to obtain advanced vanilla enchantments",
  "authors": [
    "Ryncst",
    "Athalberth"
  ],
  "contact": {
    "homepage": "https://example.com/",
    "issues": "https://github.com/AxzetStudio/ExumEnchantment/issues/",
    "sources": "https://github.com/AxzetStudio/ExumEnchantment/"
  },
  "license": "The MIT License (MIT)",
  "icon": "assets/exumenchantment/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "studio.axzet.exumenchantment.ExumEnchantment"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "studio.axzet.exumenchantment.ExumEnchantmentClient"
      }
    ]
  },
  "mixins": [
    "exumenchantment.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.8",
    "fabric": "*",
    "fabric-language-kotlin": ">=1.7.4+kotlin.1.6.21",
    "minecraft": "1.19.2",
    "java": ">=17"
  }
}
